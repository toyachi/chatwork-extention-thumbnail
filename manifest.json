{
	"manifest_version": 2,
	"name": "Chatwork thumbnail",
	"description": "",
	"version": "0.1",
	"icons": {
		"16": "images/icon_16.png",
		"48": "images/icon_48.png",
		"128": "images/icon_128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.chatwork.com/*", "https://kcw.kddi.ne.jp/*", "https://n0v1poj6m0.execute-api.ap-northeast-1.amazonaws.com/*"
			],
			"css": ["css/main.css"],
			"js": ["js/jquery.min.js", "js/base.js"],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"tabs",
		"https://*/*",
		"http://*/*"
	]
}